    "import matplotlib.pyplot as plt\n",
    "\n",
    "# 1. Define the data\n",
    "total_deaths = 1500\n",
    "male_deaths = 1200\n",
    "female_deaths = 300\n",
    "\n",
    "male_pct = male_deaths / total_deaths * 100\n",
    "female_pct = female_deaths / total_deaths * 100\n",
    "\n",
    "plt.figure(figsize=(10, 4))\n",
    "\n",
    "# 3a. Total deaths\n",
    "plt.subplot(1, 2, 1)\n",
    "plt.bar(['Deaths'], [total_deaths], color='red')\n",
    "plt.title('Total Deaths on Titanic')\n",
    "plt.ylabel('Number of People')\n",
    "plt.text(0, total_deaths + 30, str(total_deaths), ha='center')\n",
    "\n",
    "# 3b. Death percentage by gender\n",
    "plt.subplot(1, 2, 2)\n",
    "genders = ['Male', 'Female']\n",
    "percentages = [male_pct, female_pct]\n",
    "plt.bar(genders, percentages, color=['blue', 'pink'])\n",
    "plt.title('Death % by Gender')\n",
    "plt.ylabel('Percentage')\n",
    "for i, pct in enumerate(percentages):\n",
    "    plt.text(i, pct + 2, f\"{pct:.1f}%\", ha='center')\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()\n"
   ]
